{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    },
    "overrides": [{
        "files": [
            "*.svelte"
        ],
        "plugins": [
            "svelte3"
        ],
        "processor": "svelte3/svelte3",
        "rules": {
            "import/first": "off",
            "import/no-duplicates": "off",
            "import/no-mutable-exports": "off",
            "import/no-mutable-unresolved": "off",
            "no-undef": "off",
            "no-unused-vars": "off"
        }
    }]
}
